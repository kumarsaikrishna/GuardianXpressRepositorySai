
@model GuardiansExpress.Models.Entity.VehicleTypeEntity
@{
    int rCnt = 1;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Required Scripts and Styles -->

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

<!-- Edit VeicleType -->
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row flex-row-reverse">
            <div class="col-md-12 mt-3 box-margin height-card">
                <div class="card">
                    <div class="card-body">
                        <div class="modal-header">
                            <div class="d-flex align-items-center">
                                <h4 class="modal-title me-2">Edit VehicleType</h4>
                            </div>
                        </div>
                    </div>
                    <form action="~/VehicleTypeMaster/AddorUpdateVehicleType" method="post" enctype="multipart/form-data">
                        <div class="tab-content" id="myTabContent2">
                            <div class="tab-pane fade show active" id="basic-info2" role="tabpanel" aria-labelledby="info-tab2" tabindex="0">
                                <div class="modal-body pb-0 ">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="d-flex align-items-center flex-wrap row-gap-6 bg-light w-100 rounded p-3 mb-4">
                                                <div class="form-group mb-3">
                                                    <input id="Id" value="@Model.Id" name="Id" hidden>
                                                </div>
                                                <div class="form-group col-md-3 mb-3 ml-3" style="margin-right:10px;">
                                                    <label for="vehicletype" class="col-form-label"></label>
                                                    <input type="text" class="form-control" id="vehicletype" value="@Model.VehicleType" name="vehicletype" placeholder="VehicleType ">
                                                </div>
                                       
                                                <div class="modal-footer">
                                                    <a href="~/VehicleTypeMaster/VehicleTypeList" class="btn btn-danger mr-5" style="margin-right:10px;"> Cancel </a>
                                                    <button type="submit" class="btn btn-primary">Save </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
