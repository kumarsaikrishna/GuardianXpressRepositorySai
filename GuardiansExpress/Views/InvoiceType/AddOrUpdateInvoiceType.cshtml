@model GuardiansExpress.Models.Entity.InvoiceTypeMasterEntity;
<div class="pcoded-main-container">
    <div class="pcoded-wrapper">
        <div class="pcoded-content">
            <div class="pcoded-inner-content">
                <div class="main-body">
                    <div class="page-wrapper">
                        <!-- [ breadcrumb ] start -->
                        <div class="page-header">
                            <div class="page-block">
                                <div class="row align-items-center">
                                    <div class="col-md-12">
                                        <div class="page-header-title">
                                            <h5 class="m-b-10">Add Edit Invoice Type</h5>
                                        </div>
                                        <ul class="breadcrumb">
                                            <li class="breadcrumb-item"><a href="~/InvoiceType/InvoiceTypeList"><i class="feather icon-home"></i></a></li>
                                            <li class="breadcrumb-item"><a href="~/InvoiceType/InvoiceTypeList">Specializaion List</a></li>
                                            <li class="breadcrumb-item">Form Elements</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- [ breadcrumb ] end -->
                        <!-- [ Main Content ] start -->
                        <div class="row">
                            <!-- [ form-element ] start -->
                            <div class="col-sm-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h5>DoctorSpecialization</h5>
                                    </div>
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <form action="~/InvoiceType/AddOrUpdateInvoiceType" method="post">
                                                    <div class="form-group">
                                                        <input value="@Model.Id" name="Id" hidden />
                                                        <label for="exampleInputEmail1">InvoiceType </label>
                                                        <input type="text" name="InvoiceType" value="@Model.InvoiceType" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter InvoiceType Name" autocomplete="off">
                                                        @Html.ValidationMessageFor(model => model.InvoiceType, "", new { @class = "text-danger" })
                                                        @* <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> *@
                                                    </div
                                                    <div class="form-group">
                                                       
                                                        <label for="exampleInputEmail1">Status </label>
                                                        <input type="text" name="Status" value="@Model.Status" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter Status Name" autocomplete="off">
                                                        @Html.ValidationMessageFor(model => model.Status, "", new { @class = "text-danger" })
                                             
                                                    </div>
                                                    <button type="submit" class="btn btn-primary">Save</button>
                                                </form>
                                            </div>
                                        </div>

                                    </div>
                                    <!-- [ form-element ] end -->
                                    <!-- [ Main Content ] end -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

